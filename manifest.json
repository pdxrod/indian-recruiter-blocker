{
  "manifest_version": 2,
  "name": "RecruiterFilter",
  "version": "1.0",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "content_scripts" : [
    {
      "matches": ["https://mail.google.com/*", "https://inbox.google.com/*"],
      "js": ["inboxsdk.js", "jquery.js", "messageView.js", "threadView.js"]
    }
  ]
}
